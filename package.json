{
  "name": "cloudfront-update-distribution",
  "version": "1.0.0",
  "description": "Updates an AWS cloudfront distribution",
  "main": "index.js",
  "scripts": {
    "package": "ncc build src/index.ts -o dist && ncc build src/cleanup.ts -o dist/cleanup",
    "lint": "eslint --fix 'src/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frenchfounders/cloudfront-update-distribution.git"
  },
  "keywords": [
    "AWS",
    "Cloudfront",
    "Github",
    "Actions",
    "javascript"
  ],
  "author": "Chaitanya Potti",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chaitanyapotti/cloudfront-update-distribution/issues"
  },
  "homepage": "https://github.com/chaitanyapotti/cloudfront-update-distribution#readme",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@aws-sdk/client-cloudfront": "^3.410.0",
    "deepmerge": "^4.2.2"
  },
  "devDependencies": {
    "@types/node": "^14.14.31",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "@vercel/ncc": "^0.27.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "typescript": "^4.2.2"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,css,json,md}": [
      "prettier --write 'src/*'"
    ],
    "*.{js,ts}": [
      "npm run lint --"
    ]
  }
}
